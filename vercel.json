{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/cron/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/pinger",
            "schedule": "*/10 * * * *"
        }
    ]
}